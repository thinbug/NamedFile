<Window x:Class="NamedFile.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NamedFile"
        mc:Ignorable="d"
        Title="MainWindow" Height="464" Width="856">
    <Grid>
        <ListBox Name="listFiles" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,627,0"/>
        <ListBox Name="listOutputs" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="281,0,309,0"/>
        <ListBox x:Name="listRules" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="729,0,10,171"/>
        <Button Name="btnBNamed" Content="开始" HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="38" Width="111" Click="btnBNamed_Click" Margin="0,0,10,10"/>
        <Button Name="btnGetFiles" Content="添加文件" HorizontalAlignment="Right" Margin="0,0,10,116" VerticalAlignment="Bottom" Height="37" Width="75" Click="btnGetFiles_Click"/>
        <Button Name="btnRuleAdd" Content="添加规则" HorizontalAlignment="Right" Margin="0,0,10,67" VerticalAlignment="Bottom" Height="44" Width="75" Click="btnRuleAdd_Click"/>

    </Grid>
</Window>
