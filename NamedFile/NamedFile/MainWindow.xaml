<Window x:Class="NamedFile.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NamedFile"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox Name="listFiles" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,0,448,0"/>
        <ListBox Name="listOutputs" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="357,0,165,0"/>
        <Button Name="btnBNamed" Content="开始" HorizontalAlignment="Left" Margin="679,131,0,0" VerticalAlignment="Top" Height="86" Width="111" Click="btnBNamed_Click"/>
        <Button Name="btnGetFiles" Content="添加文件" HorizontalAlignment="Left" Margin="679,40,0,0" VerticalAlignment="Top" Height="56" Width="75" Click="btnGetFiles_Click"/>

    </Grid>
</Window>
